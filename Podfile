  pod 'SnapKit', '~> 3.2.0'
  pod 'SwiftIconFont'
  pod 'Pulley'
  pod 'Alamofire'
  pod 'SwiftyJSON'


  target 'RentBiTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RentBiUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
